generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @unique @default(uuid())
  name     String
  age      Int
  email    String  @unique
  password String
  phone    String
  adm      Boolean @default(false)
}

model Event {
  id               String   @id @unique @default(uuid())
  name             String
  description      String
  location         String
  date             DateTime
  age_ange         Int
  participantCount Int
  participantLimit Int
  category         String
}

model Community {
  id               String @id @unique @default(uuid())
  name             String
  description      String
  age_ange         Int
  participantCount Int
  participantLimit Int
  category         String
}
